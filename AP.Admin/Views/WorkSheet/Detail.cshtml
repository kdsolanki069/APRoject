
@{
    ViewBag.Title = "Detail";
    Layout = "~/Views/Shared/APAdminMasterPage.cshtml";
}

 
        <!-- Breadcrumbs-->
        

        <section class="content-header">
            <h1>
                Dashboard
                <small>Work Sheet</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Dashboard</a></li>
                <li><a href="#">Work Sheet</a></li>
                <li class="active">Work Sheet Detail</li>
            </ol>
            <div class="row"></div>
            <div class="col-md-12 col-xs-12 col-sm-12" style="margin-bottom: 10px;margin-top: 10px;">
