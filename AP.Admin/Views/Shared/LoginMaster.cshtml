
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>

    <link rel="stylesheet" href="~/AdminDesign/New/bower_components/bootstrap/dist/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/AdminDesign/New/bower_components/font-awesome/css/font-awesome.min.css">

    <link rel="stylesheet" href="~/AdminDesign/New/bower_components/Ionicons/css/ionicons.min.css">

    <link rel="stylesheet" href="~/AdminDesign/New/dist/css/AdminLTE.min.css">

    <link rel="stylesheet" href="~/AdminDesign/New/plugins/iCheck/square/blue.css">


</head>
<body class="hold-transition login-page">

    @RenderBody()
    <script src="~/AdminDesign/New/bower_components/jquery/dist/jquery.min.js"></script>

    <script src="~/AdminDesign/New/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>

    <script src="~/AdminDesign/New/plugins/iCheck/icheck.min.js"></script>
    <script>
        $(function () {
            $('input').iCheck({
                checkboxClass: 'icheckbox_square-blue',
                radioClass: 'iradio_square-blue',
                increaseArea: '20%' /* optional */
            });
        });
    </script>


</body>

</html>
